					type: "GET",
					url: "/jj/log/common/JJ700FK001/",
					data: { clickTime:time,ar:encodeURI("030"),nc:encodeURI("20035299"),bs:encodeURI("011"),kiss:encodeURI(""),hp:encodeURI(""),ssk2:encodeURI(""),linkType:param_1,logType:logType },
					async: false
				});
			}
		}
		//動画再生用ログ取得の可変パラメータ作成
		if(param3 == '3'){
			//可変パラムが存在しているかチェック存在している場合エンコード
			if(param1 != null){
