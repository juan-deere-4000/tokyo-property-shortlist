	    	} else {
	    		param_2 = "";
	    	}
	    	//ログタイプセット
	    	logType = 2;

			//パラメータ．プロジェクトコードが存在している場合は、プロジェクトコードにセット
			if(param2 != null){
				//ログ情報埋め込みJSP呼出
				$.ajax({
					type: "GET",
					url: "/jj/log/common/JJ700FK001/",
